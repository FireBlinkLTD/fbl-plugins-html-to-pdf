{
  "name": "@fbl-plugins/html-to-pdf",
  "version": "1.3.1",
  "description": "fbl plugin to generate PDF documents based on HTML files",
  "main": "dist/index.js",
  "scripts": {
    "commit": "npx git-cz",
    "commit:retry": "npx git-cz --retry",
    "depcheck": "yarn-check",
    "start": "node dist/src/cli.js",
    "test": "nyc --reporter=html --reporter=text mocha --exit",
    "build": "rimraf ./dist/ && tsc",
    "lint": "tslint -c tslint.json 'src/**/*.ts' 'test/**/*.ts'",
    "prettify": "prettier src/**/*.{md,ts,json} test/**/*.{md,ts,json} index.{md,ts,json} --write --config .prettierrc.yml && tslint 'src/**/*.ts' 'test/**/*.ts' 'index.ts' --fix"
  },
  "author": "FireBlink LTD",
  "license": "MIT",
  "dependencies": {
    "express": "4.17.2",
    "joi": "17.4.0",
    "portfinder": "1.0.28",
    "puppeteer": "10.0.0"
  },
  "peerDependencies": {
    "fbl": ">=1.15.1 <2.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@types/body-parser": "1.19.0",
    "@types/chai": "4.2.18",
    "@types/express": "4.17.12",
    "@types/joi": "17.2.3",
    "@types/node": "15.12.2",
    "@types/puppeteer": "5.4.3",
    "chai": "4.3.4",
    "chai-as-promised": "7.1.1",
    "commitizen": "4.2.4",
    "fbl": "1.15.1",
    "husky": "6.0.0",
    "jsonlint": "1.6.3",
    "lint-staged": "11.0.0",
    "mocha": "9.0.0",
    "mocha-typescript": "1.1.17",
    "mochawesome": "6.2.2",
    "nyc": "15.1.0",
    "pdf-parse": "1.1.1",
    "prettier": "2.3.1",
    "rimraf": "3.0.2",
    "source-map-support": "0.5.19",
    "ts-node": "10.0.0",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "typescript": "4.3.2",
    "yarn-check": "0.0.3"
  },
  "nyc": {
    "cache": false,
    "extension": [
      ".ts"
    ],
    "exclude": [
      "src/**/index.ts",
      "src/interfaces/*.ts",
      "**/*.d.ts",
      "coverage/**"
    ],
    "check-coverage": true,
    "per-file": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "include": [
      "src",
      "dist/src"
    ]
  },
  "contributors": [
    {
      "name": "Vladyslav Tkachenko",
      "email": "vlad@fireblink.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FireBlinkLTD/fbl-plugins-html-to-pdf.git"
  },
  "keywords": [
    "fireblink",
    "fbl",
    "plugin",
    "plugins",
    "pdf",
    "html",
    "renderer",
    "generate"
  ]
}
